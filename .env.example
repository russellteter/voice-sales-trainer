# Voice Sales Trainer - Environment Variables
# Copy this file to .env.local and add your API keys

# ElevenLabs API Configuration
NEXT_PUBLIC_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
NEXT_PUBLIC_ELEVENLABS_VOICE_ID=your_preferred_voice_id

# Anthropic Claude API Configuration  
NEXT_PUBLIC_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Speech-to-Text API Configuration (OpenAI Whisper)
NEXT_PUBLIC_SPEECH_TO_TEXT_API_KEY=your_openai_api_key_here

# Application Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Database Configuration (if using a database)
DATABASE_URL=your_database_connection_string_here